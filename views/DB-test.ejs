<%- include("partials/header"); -%>

<h1 class="project-header"><%=title%></h1>
<p class="container h2-resume project-container"><%= description%></p>

<%# Place to insert entries into the test database %>
<form action="/data-base-test" method="post">
	<input type="text" name="newEntry" placeholder="New Entry">
	<button type="submit" name="entryList">This is the button</button>
</form>


<table class="table">
  <thead class="thead-dark">
    <tr>
      <th scope="col">Entry</th>
    </tr>
  </thead>
  <tbody class="thead-light">
		<%# Where the entries are going to be located %>
		<% entries.forEach(function(entry){ %>
    <tr>
      <th scope="row"><%= entry.content %></th>
    </tr>
		<% }); %>
  </tbody>
</table>


	<p></p>

<%- include("partials/footer"); -%>
